syntax = "v1"

info(
    title: "type title here"
    desc: "type desc here"
    author: "type author here"
    email: "type email here"
    version: "type version here"
)


type ListBlogReq {
    Page int `form:"page"`
    Limit int `form:"limit"`
}

type ListBlogRes {
    Name string `json:"name"`
}

@server(
    group: blog
)
service blog_backend {
    @handler ListBlog
    get /list/blog (ListBlogReq) returns (ListBlogRes)
}
